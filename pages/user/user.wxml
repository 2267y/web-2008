			</view>
			<view class="f1 dfc" data-currentTab='3' bindtap="goAllorder">
				<image class="orderIcon" src="/pages/images/shouhuo.png"></image><text>待收货</text>
			</view>
			<view class="f1 dfc" data-currentTab='4' bindtap="goAllorder">
				<image class="orderIcon" src="/pages/images/pinjia.png"></image><text>待评价</text>
			</view>
		</view>
	</view>
