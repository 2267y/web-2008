<view wx:if="{{ischeck!='false'}}">
	<view class="head">
		<view class="headImg" style="border:1rpx solid #E3AAD0" bindtap="gomyMsg">
			<image style="width:100%;height:100%" src="{{imgUrl}}/customerImages/{{getPersonalInfo.autoname}}"></image>
		</view>
		<view class="headName" bindtap="gomyMsg">
			<view>{{getPersonalInfo.name}}</view>
